    <DataSet Name="IndexFragDS">
      <Query>
        <DataSourceName>SQLPTO</DataSourceName>
        <CommandText>select distinct database_id, schema_name,object_name,index_name, fragmentation, page_count, record_count, forwarded_record_count from dbo.IndexFragmentation 
where cast(fragmentation as numeric) &gt; 10.0 and page_count &gt; 8
order by fragmentation desc</CommandText>
