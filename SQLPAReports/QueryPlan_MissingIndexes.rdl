  <DataSets>
    <DataSet Name="MissingIndexesDS">
      <Query>
        <DataSourceName>SQLPTO</DataSourceName>
        <CommandText>if Exists (select * from sys.tables where name = 'MissingIndexes')
select 'MissingIndexes', s.QueryPlanName, s.statementsubtreecost, s.statementid, s.statementtext
	, i.[database], i.[Schema], i.[Table],i.Impact,i.EqualityColumns
	,i.InequalityColumns, i.IncludeColumns
from 
	qp.statements s 
		inner join qp.MissingIndexes i 
		on s.QueryPlanName = i.QueryPlanName 
order by s.queryplanname,cast(s.statementsubtreecost as decimal)desc</CommandText>
      </Query>
