    <DataSet Name="ObjectNameDS">
      <Query>
        <DataSourceName>SQLPTO</DataSourceName>
        <CommandText>select distinct ObjectName from dbo.counterdetails order by ObjectName</CommandText>
      </Query>
      <Fields>
        <Field Name="ObjectName">
          <DataField>ObjectName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CounterNameDS">
      <Query>
        <DataSourceName>SQLPTO</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ObjectName">
            <Value>=Parameters!ObjectName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct CounterName from dbo.counterdetails 
where objectname = (@ObjectName)</CommandText>
      </Query>
      <Fields>
        <Field Name="CounterName">
          <DataField>CounterName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="InstanceNameDS">
      <Query>
        <DataSourceName>SQLPTO</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ObjectName">
            <Value>=Parameters!ObjectName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CounterName">
            <Value>=Parameters!CounterName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct InstanceName from dbo.counterdetails
where objectName = (@ObjectName)  and CounterName = (@CounterName)</CommandText>
      </Query>
      <Fields>
        <Field Name="InstanceName">
          <DataField>InstanceName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CounterDataDS">
      <Query>
        <DataSourceName>SQLPTO</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ObjectName">
            <Value>=Parameters!ObjectName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CounterName">
            <Value>=Parameters!CounterName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@InstanceName">
            <Value>=Parameters!InstanceName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select cd.CounterValue,cd.RecordIndex, convert(datetime,left(CounterDateTime,23),121) as counterdatetime from dbo.CounterData cd
	inner join dbo.CounterDetails d on cd.CounterID = d.CounterID
where d.ObjectName = (@ObjectName)  and d.CounterName = (@CounterName) and (d.InstanceName = (@InstanceName) or (@InstanceName is null))
order by cd.RecordIndex</CommandText>
      </Query>
      <Fields>
        <Field Name="RecordIndex">
          <DataField>RecordIndex</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CounterDateTime">
          <DataField>counterdatetime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="CounterValue">
          <DataField>CounterValue</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
