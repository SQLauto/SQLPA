  <DataSets>
    <DataSet Name="HotObjectDS">
      <Query>
        <DataSourceName>SQLPTO</DataSourceName>
        <CommandText>select  'Objects by Cost' 
	, s.QueryPlanName
	, s.statementsubtreecost
	, s.statementid
	, s.statementtext
	, i.[database]
	, i.[Schema]
	, i.[Table]
	,i.[index]
from qp.statements s 
		inner join qp.Operators i 
		on s.QueryPlanName = i.QueryPlanName 
group by s.QueryPlanName, s.statementsubtreecost, s.statementid, s.statementtext, i.[database], i.[Schema], i.[Table], i.[index]
order by s.QueryPlanName,cast(s.statementsubtreecost as decimal) desc</CommandText>
      </Query>
